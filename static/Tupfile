include_rules

: js/*.js js/components/*.js js/components/pages/*.js |> !join |> main.js

: scss/*.css |> !join |> style.css

: $(TOP)/node_modules/classnames/index.js $(TOP)/node_modules/mersennetwister/src/MersenneTwister.js $(TOP)/node_modules/react/dist/react-with-addons.js $(TOP)/node_modules/react-dom/dist/react-dom.js $(TOP)/node_modules/react-router/umd/ReactRouter.js $(TOP)/node_modules/chart.js/Chart.js $(TOP)/node_modules/react-chartjs/dist/react-chartjs.js $(TOP)/node_modules/moment/moment.js $(TOP)/node_modules/requirejs/require.js define_libs.js |> !join |> lib.js

: $(TOP)/node_modules/jasmine-core/lib/jasmine-core/jasmine.js $(TOP)/node_modules/jasmine-core/lib/jasmine-core/jasmine-html.js $(TOP)/node_modules/jasmine-core/lib/jasmine-core/boot.js |> !join |> spec.js

: $(TOP)/node_modules/jasmine-core/lib/jasmine-core/jasmine.css |> !join |> spec.css

: foreach lib.js main.js |> uglifyjs %f > %o |> %B.min.js
